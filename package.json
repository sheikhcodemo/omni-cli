{
  "name": "omni-ai-cli",
  "version": "1.0.0",
  "description": "Universal AI CLI - Best features from Gemini CLI, OpenAI Codex, and Claude Code with multi-provider support. Works everywhere including Termux.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "omni": "dist/cli.js",
    "omni-cli": "dist/cli.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/cli.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "format": "prettier --write src",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "cli",
    "openai",
    "anthropic",
    "google",
    "gemini",
    "claude",
    "codex",
    "mcp",
    "agent",
    "termux",
    "android",
    "universal",
    "multi-provider",
    "coding-assistant",
    "ai-agent"
  ],
  "author": "sheikhcodemo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sheikhcodemo/omni-cli.git"
  },
  "homepage": "https://github.com/sheikhcodemo/omni-cli#readme",
  "bugs": {
    "url": "https://github.com/sheikhcodemo/omni-cli/issues"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.0.0",
    "@ai-sdk/google": "^1.0.0",
    "@ai-sdk/openai": "^1.0.0",
    "@ai-sdk/mistral": "^1.0.0",
    "@ai-sdk/groq": "^1.0.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "ai": "^4.0.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "conf": "^13.0.1",
    "diff": "^7.0.0",
    "dotenv": "^16.4.5",
    "execa": "^9.5.0",
    "fast-glob": "^3.3.2",
    "figures": "^6.1.0",
    "ink": "^5.0.1",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "marked": "^14.0.0",
    "marked-terminal": "^7.1.0",
    "open": "^10.1.0",
    "ora": "^8.1.0",
    "react": "^18.3.1",
    "simple-git": "^3.27.0",
    "undici": "^6.21.0",
    "zod": "^3.23.8",
    "uuid": "^10.0.0",
    "glob": "^11.0.0",
    "ollama-ai-provider": "^1.0.0",
    "@openrouter/ai-sdk-provider": "^0.4.0"
  },
  "devDependencies": {
    "@types/diff": "^5.2.0",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.0",
    "eslint": "^9.0.0",
    "prettier": "^3.3.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.0",
    "vitest": "^2.1.0",
    "@types/uuid": "^10.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32",
    "android"
  ],
  "cpu": [
    "x64",
    "arm64",
    "arm"
  ]
